           RowBox[{"{", 
            RowBox[{"startTime", ",", "1"}], "}"}]}], "}"}], "]"}], ",", 
        RowBox[{"Line", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"endTime", ",", "0"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"endTime", ",", "1"}], "}"}]}], "}"}], "]"}]}], "}"}]}]}],
     "]"}], "\[IndentingNewLine]", "\n", 
   RowBox[{"(*", 

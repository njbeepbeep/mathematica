Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Define", " ", "the", " ", "concentration", " ", "span", " ", "function", 
    " ", "M", 
    RowBox[{"(", "t", ")"}]}], "*)"}], 
  RowBox[{
   RowBox[{
    RowBox[{"M", "[", "t_", "]"}], ":=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "/", "2"}], ")"}], "-", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "/", "2"}], ")"}], " ", 
      RowBox[{"Cos", "[", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"3", "*", 
          RowBox[{"Pi", "/", "5"}]}], ")"}], "*", "t"}], "]"}]}]}]}], 
   "\[IndentingNewLine]", "\n", 
   RowBox[{"(*", 
    RowBox[{
    "Find", " ", "the", " ", "time", " ", "of", " ", "maximum", " ", 
     "concentration", " ", "span"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{"maxTime", "=", 
    RowBox[{"t", "/.", " ", 
     RowBox[{"FindMaximum", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"M", "[", "t", "]"}], ",", 
         RowBox[{"0", "<=", "t", "<=", "6"}]}], "}"}], ",", "t"}], "]"}]}]}], 
   "\[IndentingNewLine]", "\n", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
      RowBox[{"Calculate", " ", "the", " ", "50"}], "-", 
      RowBox[{"minute", " ", "class", " ", "period"}]}], ",", 
     RowBox[{
      RowBox[{"50", "/", "60"}], " ", "hours"}]}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"classDuration", "=", 
     RowBox[{"50", "/", "60"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"startTime", "=", 
     RowBox[{"maxTime", "-", 
      RowBox[{"classDuration", "/", "2"}]}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"endTime", "=", 
     RowBox[{"maxTime", "+", 
      RowBox[{"classDuration", "/", "2"}]}]}], ";"}], "\[IndentingNewLine]", 
   "\n", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
     "Plot", " ", "the", " ", "concentration", " ", "span", " ", "function", 
      " ", "M", 
      RowBox[{"(", "t", ")"}], " ", "over", " ", "6", " ", "hours"}], ",", 
     RowBox[{"with", " ", "class", " ", "time", " ", "highlighted"}]}], 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{"Plot", "[", 
    RowBox[{
     RowBox[{"M", "[", "t", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "0", ",", "6"}], "}"}], ",", 
     RowBox[{"GridLines", "->", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"startTime", ",", "endTime"}], "}"}], ",", "None"}], "}"}]}],
      ",", 
